{
  "name": "py-webdata",
  "version": "0.0.1",
  "description": "Publish data on web",
  "main": "js/index.js",
  "scripts": {
    "clean": "rimraf webdata/app",
    "build": "webpack",
    "test_reg": "python setup.py register -r pypitest",
    "test_up": "python setup.py sdist upload -r pypitest",
    "live_reg": "python setup.py register -r pypi",
    "live_up": "python setup.py sdist upload -r pypi",
    "sdist": "python setup.py sdist",
    "dev": "webpack-dev-server --config webpack.config.js",
    "debug": "node-debug -p 3333 ./node_modules/.bin/_mocha",
    "test_pub": "npm-run-all clean build test_reg test_up",
    "live_pub": "npm-run-all clean build live_reg live_up"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:walnutgeek/py-webdata.git"
  },
  "keywords": [
    "dataframe",
    "pandas",
    "data",
    "dataset",
    "R",
    "SAS",
    "python"
  ],
  "files": [
    "README.md",
    "js/*.js"
  ],
  "author": "wg@walnutgeek.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/walnutgeek/py-webdata/issues"
  },
  "homepage": "https://github.com/walnutgeek/py-webdata#readme",
  "devDependencies": {
    "codecov": "^1.0.1",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.1",
    "docco": "^0.7.0",
    "file-loader": "^0.8.5",
    "gh-pages": "^0.5.0",
    "istanbul": "^0.4.0",
    "jsdom": "^7.2.2",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "mocha": "^2.3.4",
    "mocha-jsdom": "^1.0.0",
    "mocha-lcov-reporter": "^1.0.0",
    "mocha-loader": "^0.7.1",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "uglify-loader": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.5.0",
    "jquery": "^2.1.4",
    "lodash": "^4.0.0",
    "marked": "^0.3.5",
    "wdf": "0.0.5"
  }
}
